<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
var month =2;    
var d = new Date('2020/01/01');
var date = 1;
var flag = false;    
//해당 월의 1일 요일 출력
d.setMonth(month-1);
d.setDate(1);
var gday = d.getDay();
document.writeln(d.getDay());

//해당 월의 마지막 날짜 출력
d.setMonth(month);
d.setDate(0);
var lastDay = d.getDate();
document.writeln(d.getDate());

document.writeln("<hr/>");  
document.writeln(month+"월<br/>"); 

document.write("<table border='2'>");
//달력 첫줄 요일 출력    
document.write("<tr>");
document.write("<td>일</td>");
document.write("<td>월</td>");
document.write("<td>화</td>");
document.write("<td>수</td>");
document.write("<td>목</td>");
document.write("<td>금</td>");
document.write("<td>토</td>");

//날짜 부분 출력
document.write("</tr>");
for(var j=0;j<5;j++){
    var temp=0;
	document.write("<tr>");
	for(var i =0;i<7 ;i++){
		document.write("<td>")
		 if(temp==gday){
		   flag=true;
		        }
		if(date>lastDay){
			   flag=false;
			        }
        		
		if(flag){
		document.write(date);		
		date++
		
		}
		
		
        temp++; 
		
		      
        document.write("</td>")
}
	document.write("</tr>");
}   

    

    
document.write("</table>");
</script>

