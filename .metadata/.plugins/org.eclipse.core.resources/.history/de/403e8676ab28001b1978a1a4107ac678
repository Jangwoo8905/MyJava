<script type="text/javascript">
var date=1;
var d = new date();

document.write("d.getMonth");
document.write("<table border='2'>");

for(var j=0;j<5;j++){
	document.write("<tr>");
	for(var i =0;i<7 ;i++){
		document.write("<td>"+ date +"</td>");
		date++;
	}
	document.write("</tr>");
}



document.write("</table>")



</script>


