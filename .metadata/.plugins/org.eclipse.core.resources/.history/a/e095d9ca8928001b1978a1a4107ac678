<head>
<meta charset="UTF-8">
<title>03_for연습.html</title>

<script type="text/javascript">
document.write("<table border='2'>");



for(var j=0;j<5;j++){
	document.write("<tr>");
	for(var i =1+5*j;i<6+5*j ;i++){
		document.write("<td>"+i+"</td>");
	}
	document.write("</tr>");
}


for(var j=0;j<5;j++){
	document.write("<tr>");
	for(var i =1;i<6 ;i++){
		document.write("<td></td>");
	}
	document.write("</tr>");
}



document.write("</table>")


</script>


</head>
<body>

</body>
