document.write("<table border='2'>");



for(var j=0;j<5;j++){
	document.write("<tr>");
for(var i =0;i<5 ;i++){
	document.write("<td>"+i+"</td>");
}
document.write("</tr>");
}


document.write("</table>")


</script>


